{
  "name": "FFXIV-Launcher",
  "description": "A custom FFXIV launcher",
  "author": "Josh Freeman <josh@viion.co.uk>",
  "version": "0.2.8",
  "license": "MIT",
  "devDependencies": {
    "@symfony/webpack-encore": "^0.22.4",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.4",
    "electron-packager": "^13.0.1",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0"
  },
  "main": "main.js",
  "build": {
    "appId": "ffxiv.custom.launcher",
    "win": {
      "icon": "build/logo.ico"
    },
    "mac": {
      "category": "gaming"
    }
  },
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "app": "electron .",
    "dev": "yarn run encore dev --watch"
  },
  "dependencies": {
    "keytar": "^4.3.0",
    "moment": "^2.22.2",
    "node-machine-id": "^1.1.10",
    "sha1-file": "^1.0.0",
    "uuid": "^3.3.2"
  }
}
